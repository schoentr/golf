<!DOCTYPE html>
<html lang = "en">
    <body>
        <%- include('../layout/header') %>
        <%- include('../layout/nav')%>
        <h4> Course Page</h4>
        <ul>
            <%courses.forEach (course => {%>
            <li class="course">
               <h3> <%= course.nameArray %></h3> 
               <a href="https://<%= course.webArray %>">
                    <%= course.webArray %>
               </a>
               <p><%= course.phoneArray %></p>
                <p><%= course.cityArray %>, <%= course.stateArray %></p> 

            </li>
        <% })%>
         </ul>
    </body>
   
</html>